<Window x:Class="GeneticAlgorithmTraffic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xaml="clr-namespace:Mapsui.UI.Wpf;assembly=Mapsui.UI.Wpf"
        xmlns:local="clr-namespace:GeneticAlgorithmTraffic"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <xaml:MapControl Name="MyMapControl" Height="400" Width="600" Margin="0,0,384,161" HorizontalAlignment="Right" VerticalAlignment="Bottom"></xaml:MapControl>
        <ScrollViewer  HorizontalAlignment="Left" Height="551" Margin="615,10,0,0" VerticalAlignment="Top" Width="369" >
            <TextBox x:Name="Logs" Height="538" TextWrapping="Wrap" Width="344" IsReadOnly="True"/>
        </ScrollViewer>
        <Label Content="Samochody: " HorizontalAlignment="Right" Height="25" Margin="0,442,884,0" VerticalAlignment="Top" Width="100" FontSize="13"/>
        <TextBox x:Name="CarsCount" HorizontalAlignment="Left" Height="25" Margin="115,442,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="100" FontSize="13" FlowDirection="RightToLeft" VerticalContentAlignment="Center" />
        <Button x:Name="AddCars" Content="Dodaj" HorizontalAlignment="Left" Height="25" Margin="220,442,0,0" VerticalAlignment="Top" Width="100" Click="AddCars_Click"/>
        <Button x:Name="StartSimulation" Content="Start" HorizontalAlignment="Left" Height="35" Margin="401,517,0,0" VerticalAlignment="Top" Width="195" Click="StartSimulation_Click" />
        <Button x:Name="ShowCity" Content="Centruj Kielce" HorizontalAlignment="Left" Height="25" Margin="401,442,0,0" VerticalAlignment="Top" Width="195" Click="ShowCity_Click"/>

    </Grid>
</Window>
